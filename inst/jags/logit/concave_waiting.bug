model
{
	for (i in 1:N)
	{
		u2[i] <- pow(X2[i], gamma) * pow(delta, T2[i]) - alpha * pow(T2[i] - T1[i], eta)
		u1[i] <- pow(X1[i], gamma) * pow(delta, T1[i])

		logit(p[i]) <- a * (u2[i] - u1[i])

		C[i] ~ dbern(p[i])
	}

	a ~ dunif(0.1, 9.9)
	alpha ~ dunif(0.01, 99.99)
	delta ~ dunif(0.01, 0.99)
	gamma ~ dunif(0.01, 1.99)
	eta ~ dunif(0.01, 0.99)
}
