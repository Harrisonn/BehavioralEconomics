model
{
	for (i in 1:N)
	{
		u2[i] <- (T2[i] == 0) * X2[i] + (T2[i] != 0) * X2[i] * beta * pow(delta, T2[i])
		u1[i] <- (T1[i] == 0) * X1[i] + (T1[i] != 0) * X1[i] * beta * pow(delta, T1[i])

		logit(p[i]) <- a * (u2[i] - u1[i])

		C[i] ~ dbern(p[i])
	}

	a ~ dunif(0.5, 1.5)
	beta ~ dunif(0.01, 0.99)
	delta ~ dunif(0.01, 0.99)
}
